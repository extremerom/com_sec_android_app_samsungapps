<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MasSec400" parent="@style/MasSecFontFamily">
        <item name="android:textFontWeight">400</item>
    </style>
    <style name="MasSec600" parent="@style/MasSecFontFamily">
        <item name="android:textFontWeight">600</item>
    </style>
    <style name="MasSecFontFamily">
        <item name="android:fontFamily">sec</item>

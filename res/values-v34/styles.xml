    <style name="NewSecRegular" parent="@style/NewSecLight">
        <item name="android:textFontWeight">400</item>
    </style>
    <style name="NewSecSemiBold" parent="@style/NewSecLight">
        <item name="android:textFontWeight">600</item>
    </style>
    <style name="RobotoMedium" parent="@style/NewSecSemiBold" />
    <style name="RobotoRegular" parent="@style/NewSecRegular" />

<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/search_popular_keyword_bg_color" />
            <corners android:radius="16.0dp" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <solid android:color="@color/isa_transparent" />
            <stroke android:width="1.0dp" android:color="@color/search_popular_keyword_border_color" />
            <corners android:radius="16.0dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/isa_transparent" />
            <stroke android:width="1.0dp" android:color="@color/search_popular_keyword_border_color" />
            <corners android:radius="16.0dp" />
        </shape>
    </item>
</selector>

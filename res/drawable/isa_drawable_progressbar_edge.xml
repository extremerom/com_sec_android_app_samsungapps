<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="@color/mygalaxy_edge_progress_bg_color" />
            <corners android:radius="@dimen/staffpick_progress_sector_radius" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <solid android:color="@color/isa_00145245" />
                <corners android:radius="@dimen/staffpick_progress_sector_radius" />
            </shape>
        </clip>
    </item>
</layer-list>

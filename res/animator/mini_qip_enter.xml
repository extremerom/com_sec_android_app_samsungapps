<?xml version="1.0" encoding="utf-8"?>
<set android:ordering="together"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <set android:interpolator="@anim/interpolator_mini_qip" android:ordering="together">
        <objectAnimator android:duration="350" android:startOffset="150" android:valueFrom="0.8f" android:valueTo="1.0f" android:valueType="floatType" android:propertyName="scaleY" />
        <objectAnimator android:duration="350" android:startOffset="150" android:valueFrom="0.8f" android:valueTo="1.0f" android:valueType="floatType" android:propertyName="scaleX" />
    </set>
    <set android:interpolator="@android:interpolator/linear">
        <objectAnimator android:duration="150" android:startOffset="150" android:valueFrom="0.0f" android:valueTo="1.0f" android:valueType="floatType" android:propertyName="alpha" />
    </set>
</set>
